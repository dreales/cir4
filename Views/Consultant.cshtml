@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Consultant>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
	var speciality = "Consultant Orthopaedic Surgeon and Specialist Knee Surgeon";
	var location = "Bath Hospital"
	var qualification = "MB ChB FRCS(Orth)";
}
<div class="container">
  <div class="row">
    <div class="col">
      <img src="https://www.circlehealth.co.uk/media/1401/n_bradbury_edited.jpg" alt="@Model.Content.Name is a consultant @speciality" at Circle @hospitalName />
    </div>
    <div class="col">
        <h1>@Model.Content.Name</h1>
        <p>@speciality</p>
    </div>
  </div>
</div>